@model RecruitmentProcessManagement.Models.Skill

<h2>Delete Skill</h2>

<p>Are you sure you want to delete this skill: <b>@Model.SkillName</b>?</p>

<form asp-action="DeleteSkill">
    <input type="hidden" asp-for="SkillID" />
    <button type="submit" class="btn btn-danger">Delete</button>
</form>
